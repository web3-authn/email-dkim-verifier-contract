# NEAR account ID where the EmailDkimVerifier contract will be deployed.
CONTRACT_ID=your-contract.testnet
NEAR_NETWORK_ID=testnet

# Keys for the deployer account (used by deploy/upgrade scripts)
DEPLOYER_PUBLIC_KEY=ed25519:
DEPLOYER_PRIVATE_KEY=ed25519:

# Account ID that will sign request_email_verification calls
SIGNER_ID=your-account.testnet

# Hosted Outlayer worker WASM URL (for set_outlayer_worker_wasm.sh)
OUTLAYER_WORKER_WASM_URL=https://outlayer.tatchi.xyz/workers/email-dkim/<sha>.wasm
